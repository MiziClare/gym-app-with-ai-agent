{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"Login\",\n  data() {\n    return {\n      dialogVisible: true,\n      form: {\n        role: ''\n      },\n      rules: {\n        username: [{\n          required: true,\n          message: 'Please enter your username',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: 'Please enter your password',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {},\n  methods: {\n    login() {\n      this.$refs['formRef'].validate(valid => {\n        if (valid) {\n          // 验证通过\n          this.$request.post('/login', this.form).then(res => {\n            if (res.code === '200') {\n              localStorage.setItem(\"xm-user\", JSON.stringify(res.data)); // 存储用户数据\n              this.$router.push('/'); // 跳转主页\n              this.$message.success('Login Success');\n            } else {\n              this.$message.error(res.msg);\n            }\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","dialogVisible","form","role","rules","username","required","message","trigger","password","created","methods","login","$refs","validate","valid","$request","post","then","res","code","localStorage","setItem","JSON","stringify","$router","push","$message","success","error","msg"],"sources":["src/views/Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n  <div class=\"shell\">\r\n    <div id=\"img-box\">\r\n      <img src=\"@/assets/imgs/gym-login.png\" alt=\"\" />\r\n    </div>\r\n    <form @submit.prevent=\"login\">\r\n      <div id=\"form-body\">\r\n        <div id=\"welcome-lines\">\r\n          <div id=\"w-line-1\">Gym Panel</div>\r\n          <div id=\"w-line-2\">Welcome Back</div>\r\n        </div>\r\n        <el-form :model=\"form\" :rules=\"rules\" ref=\"formRef\">\r\n          <div id=\"input-area\">\r\n            <div class=\"f-inp\">\r\n              <input \r\n                type=\"text\" \r\n                placeholder=\"Username\" \r\n                v-model=\"form.username\"\r\n              />\r\n            </div>\r\n            <div class=\"f-inp\">\r\n              <input \r\n                type=\"password\" \r\n                placeholder=\"Password\" \r\n                v-model=\"form.password\"\r\n              />\r\n            </div>\r\n            <div class=\"f-inp\">\r\n              <select id=\"role-select\" v-model=\"form.role\">\r\n                <option value=\"\" disabled selected>Your Role</option>\r\n                <option value=\"ADMIN\">Admin</option>\r\n                <option value=\"COACH\">Coach</option>\r\n                <option value=\"USER\">Member</option>\r\n              </select>\r\n            </div>\r\n          </div>\r\n          <div class=\"gdpr-consent\">\r\n            <input type=\"checkbox\" id=\"gdpr-checkbox\" required v-model=\"form.gdprConsent\" />\r\n            <label for=\"gdpr-checkbox\">\r\n              I agree to the <a href=\"#\" class=\"policy-link\">Privacy Policy</a> and\r\n              <a href=\"#\" class=\"policy-link\">Terms of Service</a>\r\n            </label>\r\n          </div>\r\n          <div id=\"submit-button-cvr\">\r\n            <button type=\"submit\" id=\"submit-button\" @click=\"login\">LOGIN</button>\r\n          </div>\r\n          <div id=\"register-link\">\r\n            <span>Don't have an account?</span>\r\n            <router-link to=\"/register\" class=\"register\">Register</router-link>\r\n          </div>\r\n        </el-form>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</div>\r\n  \r\n  <!-- <div class=\"container\">\r\n    <div style=\"width: 400px; padding: 30px; background-color: white; border-radius: 15px; box-shadow: 0 2px 12px rgba(0, 0, 0, 0.1);\">\r\n      <div style=\"text-align: center; font-size: 20px; margin-bottom: 20px; color: #333\">Welcome</div>\r\n      <el-form :model=\"form\" :rules=\"rules\" ref=\"formRef\">\r\n        <el-form-item prop=\"username\">\r\n          <el-input prefix-icon=\"el-icon-user\" placeholder=\"Enter your username\" v-model=\"form.username\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"password\">\r\n          <el-input prefix-icon=\"el-icon-lock\" placeholder=\"Enter your password\" show-password  v-model=\"form.password\"></el-input>\r\n        </el-form-item>\r\n        \r\n        <el-form-item>\r\n          <el-select v-model=\"form.role\" placeholder=\"Your Role\" style=\"width: 100%\">\r\n            <el-option label=\"Admin\" value=\"ADMIN\"></el-option>\r\n            <el-option label=\"Coach\" value=\"COACH\"></el-option>\r\n            <el-option label=\"Member\" value=\"USER\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n\r\n        <el-form-item>\r\n          <el-button style=\"width: 100%; background-color: #333; border-color: #333; color: white\" @click=\"login\">Login</el-button>\r\n        </el-form-item>\r\n        \r\n        <div style=\"display: flex; align-items: center\">\r\n          <div style=\"flex: 1\"></div>\r\n          <div style=\"flex: 1; text-align: right\">\r\n            Don't have an account? <a href=\"/register\">Register</a>\r\n          </div>\r\n        </div>\r\n      </el-form>\r\n    </div>\r\n\r\n  </div> -->\r\n\r\n\r\n  <!-- <div class=\"container\">\r\n    <div class=\"login-card\">\r\n      <div class=\"login-title\">Welcome</div>\r\n      <form @submit.prevent=\"login\">\r\n        <div class=\"form-group\">\r\n          <input\r\n            type=\"text\"\r\n            v-model=\"form.username\"\r\n            placeholder=\"Enter your username\"\r\n          />\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <input\r\n            type=\"password\"\r\n            v-model=\"form.password\"\r\n            placeholder=\"Enter your password\"\r\n          />     \r\n        </div>\r\n        <div class=\"form-group\">\r\n          <select v-model=\"form.role\">\r\n            <option value=\"\" disabled>Your Role</option>\r\n            <option value=\"ADMIN\">Admin</option>\r\n            <option value=\"COACH\">Coach</option>\r\n            <option value=\"USER\">Member</option>\r\n          </select>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <button type=\"submit\" class=\"login-btn\" @click=\"login\">Login</button>\r\n          <div class=\"register-link\">\r\n            还没有账号? <a href=\"/register\">立即注册</a>\r\n          </div>\r\n        </div>\r\n        \r\n      </form>\r\n    </div>\r\n  </div> -->\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Login\",\r\n  data() {\r\n    return {\r\n      dialogVisible: true,\r\n      form: { role: '' },\r\n      rules: {\r\n        username: [\r\n          { required: true, message: 'Please enter your username', trigger: 'blur' },\r\n        ],\r\n        password: [\r\n          { required: true, message: 'Please enter your password', trigger: 'blur' },\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n\r\n  },\r\n  methods: {\r\n    login() {\r\n      this.$refs['formRef'].validate((valid) => {\r\n        if (valid) {\r\n          // 验证通过\r\n          this.$request.post('/login', this.form).then(res => {\r\n            if (res.code === '200') {\r\n              localStorage.setItem(\"xm-user\", JSON.stringify(res.data))  // 存储用户数据\r\n              this.$router.push('/')  // 跳转主页\r\n              this.$message.success('Login Success')\r\n            } else {\r\n              this.$message.error(res.msg)\r\n            }\r\n          })\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n/* 移除默认的边距 - 注意这里不使用 scoped */\r\nhtml, body, #app {\r\n  margin: 0;\r\n  padding: 0;\r\n  width: 100vw;  /* 使用vw单位 */\r\n  height: 100vh; /* 使用vh单位 */\r\n  overflow: hidden;\r\n  position: relative; /* 添加定位上下文 */\r\n}\r\n</style>\r\n\r\n<style scoped